    "@nx/workspace": "16.6.0",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@testing-library/react": "14.0.0",
    "@types/dotenv-webpack": "^7.0.4",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "babel-jest": "^29.4.1",
    "cypress": "^12.16.0",
