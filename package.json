{
  "name": "@mf-core/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev": "nx run-many --parallel --target=serve -c development --projects=main,auth,sportbooking,management --maxParallel=10",
    "start:prod": "nx run-many --parallel --target=serve -c production --projects=main,auth,sportbooking,management --maxParallel=10",
    "start:csr": "PORT=80 serve -s dist/apps/main/exported",
    "start": "npm run start:csr",
    "build": "npm run build:prod && npm run export:prod && npm run export:post-script",
    "build:prod": "nx build main -c production && nx build auth -c production && nx build sportbooking -c production && nx build management -c production",
    "export:prod": "npx nx export main -c production && nx export auth -c production && nx export sportbooking -c production && nx export management -c production",
    "export:post-script": "mkdir -p dist/apps/main/exported/apps && cp -rf dist/apps/auth/exported dist/apps/main/exported/apps/auth && cp -rf dist/apps/sportbooking/exported dist/apps/main/exported/apps/sportbooking && cp -rf dist/apps/management/exported dist/apps/main/exported/apps/management",
    "docker:build": "npm run docker:build-image && npm run docker:clear && npm run docker:run",
    "docker:build-image": "npm run docker:builder && npm run docker:main && npm run docker:prune",
    "docker:builder": "docker build -t web-client-build-cache -f Dockerfile.builder .",
    "docker:main": "docker build --target main -t rismun/sportbooking/web-client:latest -f Dockerfile.csr .",
    "docker:run": "docker run --network=localnet -p 80:80 --name web-client  -dit --restart unless-stopped -d rismun/sportbooking/web-client",
    "docker:stop": "docker container stop web-client",
    "docker:start": "docker container start web-client",
    "docker:clear": "npm run docker:stop && docker container rm web-client",
    "docker:prune": "docker image prune && docker image ls -a",
    "docker:images": "docker image ls -a",
    "docker:containers": "docker container ls -a"
  },
  "private": true,
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@casl/react": "^3.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@iconify/icons-mdi": "^1.2.47",
    "@iconify/react": "^4.1.1",
    "@mui/lab": "^5.0.0-alpha.141",
    "@mui/material": "^5.14.3",
    "@mui/x-data-grid-premium": "^6.13.0",
    "@mui/x-date-pickers": "^6.13.0",
    "@nx/next": "16.6.0",
    "@swc/helpers": "~0.5.0",
    "apexcharts": "^3.42.0",
    "date-fns": "^2.30.0",
    "date-fns-jalali": "^2.13.0-0",
    "i18next": "^23.4.6",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "next": "13.4.1",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.2.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-to-print": "^2.14.13",
    "stylis-plugin-rtl": "^2.1.1",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@module-federation/nextjs-mf": "^7.0.6",
